<%- include("partials/header.ejs") %>

<h1>Home Page</h1>
<button class="btn primary" name="name" href="/edit">create new article</button>
<% if (locals.articleslist) { %>
<% locals.articleslist.forEach(element => { %>
  <li>
  <form action="/submit" method="POST">
    <button type="submit" class="btn btn-link p-0" name="articleName" value="<%=element%>">
    <%=element%>
    </button>
  </form>
</li>
<% }) %>
<% } %>
<%- include("partials/footer.ejs") %>